#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ninabreznik/Documents/code/play/datdot/ui/demo-maker/node_modules/.pnpm/supports-color@0.2.0/node_modules/supports-color/node_modules:/home/ninabreznik/Documents/code/play/datdot/ui/demo-maker/node_modules/.pnpm/supports-color@0.2.0/node_modules:/home/ninabreznik/Documents/code/play/datdot/ui/demo-maker/node_modules/.pnpm/node_modules:/home/ninabreznik/Documents/code/play/datdot/ui/demo-maker/node_modules:/home/ninabreznik/Documents/code/play/datdot/ui/node_modules:/home/ninabreznik/Documents/code/play/datdot/node_modules:/home/ninabreznik/Documents/code/play/node_modules:/home/ninabreznik/Documents/code/node_modules:/home/ninabreznik/Documents/node_modules:/home/ninabreznik/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/ninabreznik/Documents/code/play/datdot/ui/demo-maker/node_modules/.pnpm/supports-color@0.2.0/node_modules/supports-color/node_modules:/home/ninabreznik/Documents/code/play/datdot/ui/demo-maker/node_modules/.pnpm/supports-color@0.2.0/node_modules:/home/ninabreznik/Documents/code/play/datdot/ui/demo-maker/node_modules/.pnpm/node_modules:/home/ninabreznik/Documents/code/play/datdot/ui/demo-maker/node_modules:/home/ninabreznik/Documents/code/play/datdot/ui/node_modules:/home/ninabreznik/Documents/code/play/datdot/node_modules:/home/ninabreznik/Documents/code/play/node_modules:/home/ninabreznik/Documents/code/node_modules:/home/ninabreznik/Documents/node_modules:/home/ninabreznik/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
